var my_test_variable = 0;

if (<%= current_user.group == 0 || current_user.group == 2 %>) {
	my_test_variable = <%= @post.lowlikes %>;
}
else {
	my_test_variable = <%= @post.highlikes %>;
}

$(".like-<%=@post.id%>").html(my_test_variable + <%= @post.likes.where(user_id: current_user).size %>);
$(".heart-<%=@post.id%>").removeClass("fa-heart-o").addClass("fa-heart"); 


