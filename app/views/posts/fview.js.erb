
var my_test_variable = 0;

if (<%= current_user.group == 0 || current_user.group == 2 %>) {
	my_test_variable = <%= @fpost.highviews %>;
}
else {
	my_test_variable = <%= @fpost.lowviews %>;
}

$(".view-<%=@fpost.id%>").html(my_test_variable + <%= @fpost.fviews.where(user_id: current_user).size %>);
